{
  "name": "react-gh-like-diff",
  "version": "0.0.78",
  "description": "Generate Github like diff result in react.",
  "main": "lib/react-gh-like-diff.cjs.js",
  "module": "es/react-gh-like-diff.esm.js",
  "jsnext:main": "es/react-gh-like-diff.esm.js",
  "umd": "dist/react-gh-like-diff.umd.js",
  "scripts": {
    "test": "npm run lint && jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm run lint && jest --coverage",
    "build": "npm-run-all clean rollup minify:* size:*",
    "clean": "rimraf lib es dist",
    "rollup": "npm run clean && rollup -c",
    "minify:cjs": "uglifyjs $npm_package_main -cm toplevel -o $npm_package_main",
    "minify:esm": "babel --env=production --presets=minify $npm_package_module -o $npm_package_module",
    "minify:umd": "uglifyjs $npm_package_umd -c warnings=false -m toplevel -o $npm_package_umd",
    "size:cjs": "echo CommonJs Gziped Size: $(gzip-size $npm_package_main) | chalk yellowBright bold",
    "size:umd": "echo UMD Gziped Size: $(gzip-size $npm_package_umd) | chalk yellowBright bold",
    "size:es": "echo ESModule Gziped Size: $(gzip-size $npm_package_module) | chalk yellowBright bold",
    "size": "bundlesize",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "prepublish": "npm run build"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.spec.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/src/__tests__/stub.js"
    },
    "verbose": true,
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Peng-Jie <bivinity.pengzjie@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neighborhood999/react-gh-like-diff.git"
  },
  "keywords": [
    "compare",
    "comparison",
    "diff",
    "difference",
    "diff2html",
    "react",
    "recompose"
  ],
  "bugs": {
    "url": "https://github.com/neighborhood999/react-gh-like-diff/issues"
  },
  "license": "MIT",
  "dependencies": {
    "diff2html": "^2.3.0",
    "difflib": "^0.2.4",
    "prop-types": "^15.5.10",
    "recompose": "^0.25.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "bundlesize": "^0.14.2",
    "chalk-cli": "^4.0.0",
    "codecov": "^2.3.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.2.1",
    "gzip-size-cli": "^2.0.0",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "lint-staged": "^4.0.3",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-css-porter": "^0.1.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "uglify-js": "^3.0.27"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0",
    "react-dom": "^0.14.7 || ^15.0.0"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "bundlesize": [
    {
      "path": "./lib/react-gh-like-diff.cjs.js",
      "maxSize": "3.5 kB"
    }
  ]
}

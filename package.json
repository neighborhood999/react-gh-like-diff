{
  "name": "react-gh-like-diff",
  "version": "0.0.1",
  "description": "Generate Github like diff result.",
  "main": "lib/react-gh-like-diff.cjs.js",
  "module": "es/react-gh-like-diff.esm.js",
  "jsnext:main": "es/react-gh-like-diff.esm.js",
  "umd": "dist/react-gh-like-diff.umd.js",
  "scripts": {
    "build": "npm-run-all clean rollup minify:* size:*",
    "clean": "rimraf lib es dist",
    "rollup": "npm run clean && rollup -c",
    "minify:cjs": "uglifyjs $npm_package_main -cm -o $npm_package_main",
    "minify:esm": "babel --env=production --presets=minify $npm_package_module -o $npm_package_module",
    "minify:umd": "uglifyjs $npm_package_umd -cm -o $npm_package_umd",
    "size:cjs": "echo CommonJs Gziped Size: $(gzip-size $npm_package_main) | chalk greenBright bold",
    "size:umd": "echo UMD Gziped Size: $(gzip-size $npm_package_umd) | chalk greenBright bold",
    "size:es": "echo ESModule Gziped Size: $(gzip-size $npm_package_module) | chalk greenBright bold",
    "lint": "eslint src",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Peng-Jie <bivinity.pengzjie@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "diff2html": "^2.3.0",
    "difflib": "^0.2.4",
    "prop-types": "^15.5.10",
    "recompose": "^0.24.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "chalk-cli": "^4.0.0",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.2.1",
    "gzip-size-cli": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.47.4",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-css-porter": "^0.1.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "uglify-js": "^3.0.27"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0",
    "react-dom": "^0.14.7 || ^15.0.0"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ]
}
